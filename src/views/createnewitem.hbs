    <title>Todo</title>
</head>
<body>
 {{>header}}
    <div class="container">


<div class="list">
  <h1>Create new item</h1>
  <div class="row">
         
              <h3 class="col-md-4">Title</h3>
              <h3 class="col-md-4">Description</h3>
              <h3 class="col-md-4">Due Date</h3>
              
      </div>
      <form action="" method="POST">
      <div class="row">
         
        <input type="text" class="col-md-4" name="tittle" placeholder="Item" required>
        <input type="text" class="col-md-4" name="description" placeholder="Description">
        <input type="date" class="col-md-4" name="duedate" placeholder="Date due" required>
        <script>dayjs(date).format('DD/MM/YYYY')</script>
</div>
<button type="submit" class="btn btn-primary">Create item</button>
</form>
  </div>
 
</div>
{{>footer}}

      
</body>
</html>